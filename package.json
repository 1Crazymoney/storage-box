{
  "name": "storagebox",
  "version": "0.1.0-canary.0",
  "description": "A memory-based keyâ€“value storage with fancy utils for Node.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check \"**/*.{ts,md}\"",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm run format:check && pnpm run typecheck && pnpm run build"
  },
  "packageManager": "pnpm@8.12.0",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "license": "GPL-3.0",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "repository": {
    "type": "git",
    "url": "https://github.com/shahradelahi/storage-box.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
